{
  "name": "project-potion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'pnpm run server:dev'",
    "server:dev": "tsx watch --clear-screen=false --tsconfig tsconfig.server.json src/server/index.ts",
    "build": "tsc && vite build",
    "build:server": "esbuild src/server/index.ts --bundle --platform=node --tsconfig=tsconfig.server.json --outfile=./dist/server/index.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^3.0.1",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "express": "^4.18.2",
    "lucia": "^2.4.2",
    "lucide-react": "^0.274.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.8",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-node": "^0.34.3"
  }
}
